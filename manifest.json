{
  "manifest_version": 3,
  "name": "Google Drive File Renamer",
  "version": "1.0",
  "description": "Rename files in Google Drive.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  }
}

  